<h3>Song Form</h3>

<%= form_for (@song) do |f| %>
    <label for="song_name">Song name: </label><br>
    <%= f.text_field :name %><br>

    <label for="song_artist_id">Artist id: </label><br>
    <%= f.text_field :artist_id %><br>

    <label for="song_genre_id">Genre id: </label><br>
    <%= f.text_field :genre_id %><br>

    <%= f.submit %>
<% end %>